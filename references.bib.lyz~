@article{williams_leasing_2011,
	Abstract = {A fresh examination of six inscribed fragments (Agora {XIX} L6 a--f ) previously attributed to the first of a series of stelai recording civic leases of sacred land in late-4th-century Athens reveals that they belong to four separate stelai, only one of which can be dated to 343/2 b.c. The publication of the leases was linked to a reorganization of sacred finances that included the amalgamation
of the treasuries of Athena and the Other Gods, ca. 346/5. The new reconstruction challenges previous estimates of the extent of Athenian sacred property and the assumption that subsequent lists (Agora {XIX} L9--12, L14)
were produced only at 10-year intervals.},
	Author = {Williams, Arden},
	Date = {2011},
	Date-Added = {2018-03-22 19:45:25 +0000},
	Date-Modified = {2018-03-22 19:45:25 +0000},
	File = {Williams - 2011 - Leasing of Sacred Land in 4th-Century Athens A Re.pdf:/Volumes/Marsh_Files/Zotero_Library/WilliamsA/Williams - 2011 - Leasing of Sacred Land in 4th-Century Athens A Re.pdf:application/pdf},
	Journaltitle = {Hesperia},
	Note = {00011},
	Pages = {261--286},
	Title = {Leasing of Sacred Land in 4th-Century Athens: A Reassessment of Six Inscribed Fragments},
	Url = {http://www.ascsa.edu.gr/pdf/uploads/hesperia/hesperia.80.2.0261.pdf},
	Urldate = {2018-03-15},
