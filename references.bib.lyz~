@article{barnes2012reopkelecture,
  title = {Reopke {{Lecture}} in {{Economic Geography}}: {{Notes}} from the {{Underground}}: {{Why}} the {{History}} of {{Economic Geography Matters}}: {{The Case}} of {{Central Place Theory}}},
  volume = {88},
  issn = {00130095},
  url = {http://uri.idm.oclc.org/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=a9h&AN=70163840&site=ehost-live&scope=site},
  doi = {10.1111/j.1944-8287.2011.01140.x},
  shorttitle = {Reopke {{Lecture}} in {{Economic Geography}}},
  abstract = {The discipline of Anglo-American economic geography seems to care little about its history. Its practitioners tend toward the 'just do it' school of scholarship, in which a concern with the present moment in economic geography subordinates all else. In contrast, I argue that it is vital to know economic geography's history. Historical knowledge of our discipline enables us to realize that we are frequently 'slaves of some defunct' economic geographer; that we cannot escape our geography and history, which seep into the very pores of the ideas that we profess; and that the full connotations of economic geographic ideas are sometimes purposively hidden, secret even, revealed only later by investigative historical scholarship. My antidote: 'notes from the underground,' which means a history of economic geography that delves below the reported surface. This history is often subversive, contradicting conventional depictions; it is antirationalist, querying universal (timeless) foundations; it seeks out deliberately hidden and buried economic geographic practices, relying on sources literally found underground-personal papers and correspondence stored in one subterranean archive or another. To exemplify the importance of notes from the underground, I present an extended case study-the 20th-century development of central place theory, associated with two economic geographers: the German, Walter Christaller (1893-1969), and the American, Edward L. Ullman (1912-76).},
  number = {1},
  journaltitle = {Economic Geography},
  shortjournal = {Economic Geography},
  urldate = {2018-03-29},
  date = {2012-01},
  pages = {1--26},
  keywords = {BARNES; Trevor J.,CENTRAL places,CHRISTALLER; Walter,ECONOMIC geography,GERMANY,HISTORY of geography,ULLMAN; Edward L.},
  author = {Barnes, Trevor J.},
  file = {/Volumes/Marsh_Files/Zotero_Library/BarnesT/Barnes - 2012 - Reopke Lecture in Economic Geography Notes from t.pdf},
  note = {00017}
